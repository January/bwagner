method run () {
   while (!isFlower(AHEAD)) {
      if (isClear(RIGHT)) {
         turn(RIGHT);
         hop();
      }
      if (isClear(LEFT) && !isClear(AHEAD)) {
         turn(LEFT);
         
      }
      if (!isClear(AHEAD) && !isClear(LEFT) && !isClear(RIGHT)) {
         turn(LEFT);
         turn(LEFT);
      
      }
      hop();
   }
}
