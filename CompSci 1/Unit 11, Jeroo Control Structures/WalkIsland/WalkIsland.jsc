@Java
method walk() {
   while(!isFlower(AHEAD)) {
      if (!isClear(RIGHT)) {
         hop();
      }
      if (isClear(RIGHT)) {
         turn(RIGHT);
         hop();
      }
      if (isWater(AHEAD) && isWater(RIGHT)) {
         turn(LEFT);
      }
   }

}
